@import "std/strings"
var x = "Tim is Awesome"

assert $x.toUpper == "TIM IS AWESOME"
assert $x.toLower == "tim is awesome"
assert $x.startsWith("Tim")
assert $x.endsWith("some")
assert $x.replace("Awesome", "Great!") == "Tim is Great!"
assert $x.contains("Awesome")

assert parseInt("100") == 100
assert parseFloat("100.99") == 100.99
assert parseBool("true")
assert parseBool("false") == false
assert format("$1 is $2", ["Tim", "Awesome"]) == $x